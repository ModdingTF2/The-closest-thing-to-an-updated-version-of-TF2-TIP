"VertexlitGeneric"
{
	"$baseTexture" "models/props_halloween/flask"
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"
	
	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" "1"
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.25 1 4]"

	"$rimlight" "1"
	"$rimlightexponent" "60"
	"$rimlightboost" ".5"
	// Cloaking
	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"weapon_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}
	}
}
