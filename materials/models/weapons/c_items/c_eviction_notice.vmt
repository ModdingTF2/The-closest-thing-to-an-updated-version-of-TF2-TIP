"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items\c_eviction_notice"
	
	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" "1.0"	
	"$phongfresnelranges"	"[.25 .5 1]"
	"$basemapalphaphongmask" "1"
	"$basealphaenvmapmask" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[0.9 0.5 0.2]"
//	"$lightwarptexture" "models\weapons\c_items\c_fists_of_steel_lightwarp"
//	"$phongwarptexture"  "models\weapons\c_items\c_fists_of_steel_phongwarp"
	"$rimlight" "1"
	"$rimlightexponent" "10"	
	"$rimlightboost" ".5"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}
