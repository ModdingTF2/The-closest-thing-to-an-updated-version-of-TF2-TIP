	"$glowcolor" "1"

	"360?$color2" "[ 0.9 0.8 0.8 ]"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}

		Sine
		{
			"resultVar"	"$selfillumtint" 
		        "sineperiod"	2
			"sinemin"       0
			"sinemax"	10
		}

		Sine
		{
			"resultVar"	"$envmaptint" 
		        "sineperiod"	2
			"sinemin"       0
			"sinemax"	.35
		}

	}
}