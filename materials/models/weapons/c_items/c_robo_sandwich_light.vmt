"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_robo_sandwich"
	"$basemapalphaphongmask" 1
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$selfillum" 1
	
	//phong properties
	"$phong"	"1"
	"$phongboost"	"5"
	"$phongexponent"	"55"
	"$phongfresnelranges"	"[1 2 2.5]"
	"$rimlight"	"1"
	"$rimlightexponent"	"35"

	"$envmap" "env_cubemap"
//	"$envmaptint" "[.5 .5 .5]"
