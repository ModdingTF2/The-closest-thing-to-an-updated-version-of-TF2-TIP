"VertexLitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_flamethrower_gold"
	
	"$phong" "1"
	"$phongexponent" "175"
	"$phongboost" "5"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[1.5 .5 3]"
	"$halflambert" "1"
	
	// Begin GOLD look

	"$envmap" "cubemaps/cubemap_gold001"	
	//"$envmap" "effects/saxxy/saxxy_gold"
	"$envmaptint" "[2.5 2.5 1.15]"
	
	// End GOLD look

	"$tempvar" "0"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "2"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".5"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}

