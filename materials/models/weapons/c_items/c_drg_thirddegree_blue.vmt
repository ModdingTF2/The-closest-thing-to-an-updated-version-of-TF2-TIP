"VertexlitGeneric"
{
	"$basetexture"	"models\weapons\c_items\c_drg_thirddegree_blue"
	"$bumpmap" "models\weapons\c_items\c_drg_thirddegree_normal"

	"$halflambert" "1"

	"$selfillum" "1"

	"$normalmapalphaenvmapmask" 1

	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "10"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$phongwarptexture"  "models\lightwarps\weapon_lightwarp"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" ".5"

	"360?$color2" "[ 0.9 0.8 0.8 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}


	}
}
