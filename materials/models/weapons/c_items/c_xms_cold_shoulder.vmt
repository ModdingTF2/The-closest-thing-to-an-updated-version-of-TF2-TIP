"VertexlitGeneric"
{
	"$basetexture" "models/weapons/c_items/c_xms_cold_shoulder"
        "$bumpmap" "models/weapons/c_items/c_xms_cold_shoulder_normal"

	"$translucent" 1

	"$envmap" "env_cubemap"
	"$envmaptint" "[.9 .9 1]"
	
	"$surfaceprop" "Glass"

		"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "5"	
	
	"$phongfresnelranges"	"[.3 .5 3]"


	"$basephongexponent" "15"
	"$basephongboost" "5"

	
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".5"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}


}
