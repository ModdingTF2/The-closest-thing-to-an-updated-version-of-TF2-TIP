"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items/c_battalion_bugle"
	"$bumpmap" "models\weapons\c_items/c_battalion_bugle_normal"
	
	"$phong" "1"
	"$phongexponent" "6"
	"$phongboost" "0.4"	
	"$phongfresnelranges" "[1.5 .5 3]"
	"$halflambert" "0"
	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" ".5"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}
