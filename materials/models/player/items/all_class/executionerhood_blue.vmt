"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/all_class/executionerhood"
	"$bumpmap" "models/player/items/all_class/executionerhood_normal"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6

	"$yellow" "0"
	"$lightwarptexture" "models\player\pyro\pyro_lightwarp"

	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" ".1"
	"$phongfresnelranges" "[.1 .1 1]"

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" ".5"

	"$blendtintbybasealpha" "1"
	"$blendtintcoloroverbase" "1"
	
	"$colortint_base" "{44 52 62}"
	"$colortint_tmp" "{0 0 0}"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"weapon_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}
		"ItemTintColor"
  		{
   			"resultVar" "$colortint_tmp"
  		}
  		"SelectFirstIfNonZero"
  		{
   			"srcVar1" "$colortint_tmp"
   			"srcVar2" "$colortint_base"
   			"resultVar" "$color2"
  		}
	}
}




