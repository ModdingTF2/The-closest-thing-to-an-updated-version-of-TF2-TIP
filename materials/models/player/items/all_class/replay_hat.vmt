"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/all_class/replay_hat"
	"$bumpmap" "models/player/items/all_class/replay_hat_normal" 
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"
	
	
