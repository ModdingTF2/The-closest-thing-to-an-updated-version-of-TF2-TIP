"VertexlitGeneric"
{
	"$basetexture" "models/player/items/all_class/hm_disguisehat"
	"$bumpmap" "models/player/items/all_class/hm_disguisehat_normal"
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .0025
	"$detailblendmode" 6
	"$yellow" "1"
	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "3"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$normalmapalphaphongmask" "1"
	
 	"$blendtintbybasealpha" "1" 
 	"$blendtintcoloroverbase" "0"
 
 	"$color2" "{ 68 56 48}" //red
//	"$color2" "{ 96 141 170}"
	"$colortint_base" "{ 68 56 48}"
	"$colortint_tmp" "[0 0 0]" 
	
	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" ".5"										// Boost for ambient cube component of rim lighting
	
	"$glowcolor" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"weapon_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
 		"ItemTintColor"
 		{
 			"resultVar" "$colortint_tmp"
 		}
 		"SelectFirstIfNonZero"
 		{
 			"srcVar1" "$colortint_tmp"
 			"srcVar2" "$colortint_base"
 			"resultVar" "$color2"
 		}
	}
}
