"VertexlitGeneric"
{
	"$basetexture" "models/player/items/all_class/etf2l_highlander_part"
	"$bumpmap" "models/effects/flat_normal" 
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"

	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "6"

	"$phongfresnelranges"	"[.25 1 4]"
	"$basemapalphaphongmask" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[1.6 1.6 1.6]"
	"$basealphaenvmapmask" 1 



	"$rimlight" "1"												
	"$rimlightexponent" "4"											
	"$rimlightboost" ".5"										

	

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"weapon_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}
	}
}
