"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/all_class/scarf_soccer_blue"
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"
	

	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"

	"$rimlight" "1"
	"$rimlightexponent" "20"
	"$rimlightboost" ".5"
	$nocull "1"
	
	// Cloaking
	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"weapon_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
	}
}
