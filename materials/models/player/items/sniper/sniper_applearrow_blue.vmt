"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/sniper/sniper_applearrow"

	"$bumpmap" "models/player/items/sniper/sniper_applearrow_normal"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6

    	"$blendtintbybasealpha" "1" 
    	"$blendtintcoloroverbase" "0"   // between 0-1 determines how much blended by tinting vs. replacing the color

	"$yellow" "0"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "5"
	"$phongfresnelranges" "[.25 .5 1]"
	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" ".5"

	//paint color
	"$colortint_base" "{170 162 50}"
	"$colortint_tmp" "[0 0 0]" 

	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"weapon_invis"
		{
		}

		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}

		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}

		"YellowLevel"
		{
			"resultVar" "$yellow"
		}

		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
	}
}
