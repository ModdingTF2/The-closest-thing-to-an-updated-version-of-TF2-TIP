"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/pyro/smnc_pyro"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6

	"$yellow" "0"
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"

	"$phong" "1"
	"$phongexponent" "45"
	"$phongboost" "5"
	"$phongfresnelranges" "[.25 .5 1]"
	"$basemapalphaphongmask" "1"
	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" ".5"

	"$baseAlphaEnvMapMask" "1" 
	"$envmap" "env_cubemap"
	"$envmaptint" "[.75 .75 .75]"
	"$envmapcontrast" "1"
	"$envmapsaturation" "1.0"
	"$multipass" "1"

	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"weapon_invis"
		{
		}

		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}

		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}

		"YellowLevel"
		{
			"resultVar" "$yellow"
		}

		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}
