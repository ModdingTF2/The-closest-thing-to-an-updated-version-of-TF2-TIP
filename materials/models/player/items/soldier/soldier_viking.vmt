"VertexLitGeneric"
{
	$baseTexture "models/player/items/soldier/soldier_viking.vtf"
	"$bumpmap" "models/player/items/soldier/soldier_viking_normal.vtf"
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "2"
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"	
	"$phongfresnelranges"	"[.25 1 4]"

 	"$blendtintbybasealpha" "1" 
 	"$blendtintcoloroverbase" "1"  

	"$colortint_base" "{205 23 21}"   
	"$colortint_tmp" "[0 0 0]" 

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" ".5"										// Boost for ambient cube component of rim lighting

	// Cloaking
	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"weapon_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}
  		"ItemTintColor"
  		{
   			"resultVar" "$colortint_tmp"
  		}
  		"SelectFirstIfNonZero"
 		{
   			"srcVar1" "$colortint_tmp"
   			"srcVar2" "$colortint_base"
   			"resultVar" "$color2"
  		}

	}
}

