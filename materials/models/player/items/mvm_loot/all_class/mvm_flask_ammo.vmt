"VertexLitGeneric"
{
	"$basetexture"	"models\player\items\mvm_loot\all_class\mvm_flask_ammo"
	
	"$phong" "1"
	"$phongexponent" "50"
	"$phongboost" ".1"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"	
	"$phongfresnelranges"	"[1.5 .5 3]"
	"$halflambert" "1"

	"$tempvar" "0"

	"$basephongexponent" "15"
	"$basephongboost" ".25"

	"$commadd_phongexponent" "30"
	"$commadd_phongboost" 	"1"
	"$commweapon"		"0"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent"	 "4"				
	"$rimlightboost" ".5"

	"$glowcolor" "1"

	"$selfillum" "1"
	"$selfillumtint" "[1 1 1]"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"weapon_invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"CommunityWeapon"
		{
			"resultVar" "$commweapon"
		}
		"Multiply"
		{
			"srcVar1"  "$commweapon"
			"srcVar2"  "$commadd_phongexponent" 
			"resultVar" "$tempvar" 
		}
		"Add"
		{
			"srcVar1"  "$tempvar" 
			"srcVar2"  "$basephongexponent" 
			"resultVar" "$phongexponent" 
		}
		"Multiply"
		{
			"srcVar1"  "$commweapon"
			"srcVar2"  "$commadd_phongboost" 
			"resultVar" "$tempvar" 
		}
		"Add"
		{
			"srcVar1"  "$tempvar" 
			"srcVar2"  "$basephongboost"
			"resultVar" "$phongboost" 
		}

	}
}
