"VertexLitGeneric"
{
	"$basetexture"	"models\player\items\mvm_loot\all_class\fob_illum"
	
	"$phong" "1"
	"$phongexponent" "50"
	"$phongboost" ".1"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"	
	"$phongfresnelranges"	"[1.5 .5 3]"
	"$halflambert" "1"

	"$tempvar" "0"
	
	"$commweapon"		"0"
	
	"$commadd_phongexponent" "30"
	"$commadd_phongboost" 	"22.5"

	"$basephongexponent" "15"
	"$basephongboost" "5"

	"$colortint_base" "{255 0 0}"
	"$colortint_tmp" "[0 0 0]" 
	"$color2" "{255 0 0}"

	">=DX90"
	{
	"$selfillum" "1"
	"$selfillumtint" "[1 1 1]"
	}
	
	"$rimlight" "1"						
	"$rimlightexponent" "4"				
	"$rimlightboost" ".5"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"weapon_invis"
		{
		}
		"vm_invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"CommunityWeapon"
		{
			"resultVar" "$commweapon"
		}
		"Multiply"
		{
			"srcVar1"  "$commweapon"
			"srcVar2"  "$commadd_phongexponent" 
			"resultVar" "$tempvar" 
		}
		"Add"
		{
			"srcVar1"  "$tempvar" 
			"srcVar2"  "$basephongexponent" 
			"resultVar" "$phongexponent" 
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		

	}
}
