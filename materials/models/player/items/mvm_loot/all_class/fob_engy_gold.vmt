"VertexLitGeneric"
{
	"$basetexture"	"models/player/items/mvm_loot/all_class/fob_engy_gold"
//	"$bumpmap" "models/weapons/c_items/c_saxxy_normal"	
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"

	"$basephongexponent" "25"
	"$basephongboost" ".5"

	">=DX90"
	{
		"$envmap" "effects/saxxy/saxxy_gold"
		"$envmaptint" "[.8 .8 .8]"
	}

	"$commadd_phongexponent" "40"
	"$commadd_phongboost" 	"22.5"
	"$commweapon"		"0"
	
//	
"$normalmapalphaenvmapmask" "1"
	"$basemapalphaphongmask"	"1"

	"$phong" "1"
	"$phongexponent" "60"
	"$phongboost" "5"
	"$phongfresnelranges"	"[.2 1 4]"
		
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"

	"$glowcolor" "1"
	"$tempvar" "0"

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" ".5"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"weapon_invis"
		{
		}
		"vm_invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"CommunityWeapon"
		{
			"resultVar" "$commweapon"
		}
		"Multiply"
		{
			"srcVar1"  "$commweapon"
			"srcVar2"  "$commadd_phongexponent" 
			"resultVar" "$tempvar" 
		}
		"Add"
		{
			"srcVar1"  "$tempvar" 
			"srcVar2"  "$basephongexponent" 
			"resultVar" "$phongexponent" 
		}
	}

}

